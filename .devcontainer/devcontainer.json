{
  "name": "ROS2 Secure Nodes Multi-Container",
  "dockerComposeFile": "docker-compose.yml",
  "service": "publisher",
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "ms-python.python",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "postCreateCommand": "echo 'Dev container ready. Use docker-compose logs to see both nodes.'"
}
